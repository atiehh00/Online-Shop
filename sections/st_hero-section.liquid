<script
  src="https://cdn.shopify.com/s/files/1/0799/5880/2740/files/verifier.js"
  data-section-id="{{ section.id }}"
  defer
></script>

<div class="st_not-found"></div>

<style>
  .st_wrapper div:empty {
    display: unset;
  }

  .st_hero-section-{{ section.id }} {
    width: 100%;
    background: #fff;
    {% unless section.settings.full_width %}
    padding-inline: 15px;
    {% endunless %}
    padding-top: {{ section.settings.section_padding_top }}px;
    padding-bottom: {{ section.settings.section_padding_bottom }}px;
    overflow: hidden;
  }

  .st_hero-section_wrapper-{{ section.id }} {
    display: flex;
    {% unless section.settings.full_width %}
    max-width: {{ section.settings.section_max_width }}px;
    margin: 0 auto;
    {% endunless %}
    {% if section.settings.background_image_checkbox %}
    background: {{section.settings.bg_color}};
    {% endif %}
    justify-content: center;
    height: 100%;
    {% if section.settings.desktop_layout == 'media_right' %}
    flex-direction: row-reverse;
    {% endif %}
    {% unless section.settings.full_width %}
    border-radius: {{ section.settings.border_radius }}px;
    {% endunless %}
    overflow: hidden;
  }

  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left_gradient {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-size: cover;
    background-position: center;
  }

  .st_hero-section_right_text_title {
    text-align: left;
  }

  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_right {
    width: 50%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    {% if section.settings.desktop_layout == 'media_right' %}
    align-items: flex-end;
    {% else %}
    align-items: flex-start;
    {% endif %}
    padding: {{ section.settings.content_padding }}px;
    min-height: {{section.settings.section_height}}px;
  }

  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_right_text {
    display: flex;
    max-width: 800px;
    width: 100%;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    gap: {{section.settings.content_gap}}px;
  }

  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left{
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 50%;
  }

  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left_wrapper {
    position: absolute;
    top: 50%;
    {% if section.settings.desktop_layout == 'media_right' %}
    left: 0;
    {% else %}
    right: 0;
    {% endif %}
    transform: translateY(-50%);
    width: 100%;
    height: 100%;
    min-height: 100%;
    overflow: hidden;
  }

  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left .background-img {
      height: 100%;
      object-fit: cover;
      width: 100% !important;
  }

  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left .overlay-img{
    height: 100%;
    max-width: 800px;
    object-fit: contain;
    width: 100% !important;
  }


  {% if section.settings.overlay_image_checkbox %}
  .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left .background-img {
      position: relative; /* Background image stays at the bottom */
      z-index: 1;
  }
  {% endif %}

  {% if section.settings.overlay_image_checkbox %}
   .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left .overlay-img {
      position: absolute;
      top: 50%;
      {% if section.settings.desktop_layout == 'media_right' %}
      left: 0;
      {% else %}
      right: 0;
      {% endif %}
      transform: translateY(-50%);
      z-index: 2; /* Higher z-index makes it appear on top */
      pointer-events: none; /* Optional: Ensures clicks pass through */
  }
  {% endif %}

  {% if section.settings.overlay_image_checkbox %}
    .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left{
      position: relative;
    }
  {% endif %}

  /* Title block styles */
  {% for block in section.blocks %}
    {% if block.type == 'title' %}
      {{ block.settings.title_font | font_face: font_display: 'swap' }}

      .st_hero-section_right_text_title-{{ block.id }}, .st_hero-section_right_text_title-{{ block.id }} .highlight {
        font-size: {{ block.settings.title_size }}px;
        line-height: {{ block.settings.title_line_height }};
        color: {{ block.settings.title_color }};
        margin: 0; /* Remove margin since we're using flex gap */
        {% if block.settings.title_font %}
          font-family: {{ block.settings.title_font.family }}, {{ block.settings.title_font.fallback_families }} !important;
          font-weight: {{ block.settings.title_font.weight }} !important;
        {% endif %}
      }

      .st_hero-section_right_text_title-{{ block.id }} .highlight {
        color: {{ block.settings.highlight_color }} !important;
        display: inline; /* Make div behave like inline element */
      }

      @media screen and (max-width: 768px) {
        .st_hero-section_right_text_title-{{ block.id }}, .st_hero-section_right_text_title-{{ block.id }} .highlight {
          font-size: {{ block.settings.mobile_title_size }}px;
        }
      }
    {% endif %}
  {% endfor %}

  /* Description block styles */
  {% for block in section.blocks %}
    {% if block.type == 'description' %}
      {{ block.settings.heading_font | font_face }}

      .st_hero-section_right_text_description-{{ block.id }}, .st_hero-section_right_text_description-{{ block.id }} * {
        font-size: {{block.settings.font_size}}px;
        color: {{block.settings.font_color}};
        font-family: {{block.settings.heading_font.family}}, {{block.settings.heading_font.fallback_families}} !important;
        line-height: {{block.settings.line_height}}%;
        font-weight: {{block.settings.heading_font.weight}};
        font-style: {{block.settings.heading_font.style}};
        margin: 0;
      }

      .st_hero-section_right_text_description-{{ block.id }} st {
        color: {{block.settings.highlight_font_color}};
      }

      @media screen and (max-width: 768px) {
        .st_hero-section_right_text_description-{{ block.id }}, .st_hero-section_right_text_description-{{ block.id }} * {
          font-size: {{ block.settings.mobile_font_size }}px;
        }
      }
    {% endif %}
  {% endfor %}

  /* Star Rating block styles */
  {% for block in section.blocks %}
    {% if block.type == 'star_rating' %}
      /* Star rating styles */
      {{ block.settings.heading_font | font_face }}

      .st_hero-section_right_text_star_rating-{{ block.id }} span,
      .st_hero-section_right_text_star_rating-{{ block.id }} span *,
      .st_hero-section_right_text_star_rating-{{ block.id }} span strong {
        font-size: {{ block.settings.font_size }}px;
        color: {{ block.settings.font_color }};
        font-family: {{block.settings.heading_font.family}}, {{block.settings.heading_font.fallback_families}};
        font-weight: {{block.settings.heading_font.weight}};
        font-style: {{block.settings.heading_font.style}};
      }

      .st_hero-section_right_text_star_rating-{{ block.id }} {
        display: flex;
        justify-content: center;
        align-items: center;
        gap: {{ block.settings.text_gap_size }}px;
      }

      .st_hero-section_right_text_star_rating-stars-{{ block.id }} {
        display: flex;
        justify-content: center;
        align-items: center;
        gap: {{ block.settings.star_gap_size }}px;
      }

      .rating-{{ block.id }} {
        width: calc({{ block.settings.star_size }}px * 5.5);
        max-height: {{ block.settings.star_size }}px;
      }

      .rating__background-{{ block.id }} {
        fill: {{ block.settings.star_color_background }};
        stroke: none;
        stroke-width: 0;
        height: 100%;
        width: 100%;
      }

      .rating__value-{{ block.id }} {
        fill: {{ block.settings.star_color }};
        height: 100%;
      }

      @media screen and (max-width: 768px) {
        .st_hero-section_right_text_star_rating-{{ block.id }} span,
        .st_hero-section_right_text_star_rating-{{ block.id }} span * {
          font-size: {{ block.settings.mobile_font_size }}px;
        }

        .rating-{{ block.id }} {
          width: calc({{ block.settings.star_size }}px * 5.5 / 1.25);
          max-height: calc({{ block.settings.star_size }}px / 1.25);
        }
      }
    {% endif %}
  {% endfor %}

  /* Icon and Text block styles */
  .st_hero-section_right_text_features-container {
    list-style: none !important;
    margin: 0;
    padding: 0 !important;
    display: flex;
    flex-direction: column;
    gap: 5px;
    width: 100%;
  }

  {% for block in section.blocks %}
    {% if block.type == 'icon_and_text' %}
      {{ block.settings.feature_font | font_face }}

      .st_hero-section_right_text_feature-item--{{ block.id }} {
        display: flex;
        align-items: center;
        margin: 0;
        font-size: {{ block.settings.feature_size }}px;
        color: {{ block.settings.feature_color }};
        {% if block.settings.feature_font %}
          font-family: {{ block.settings.feature_font.family }}, {{ block.settings.feature_font.fallback_families }};
          font-weight: {{ block.settings.feature_font.weight }};
        {% endif %}
      }

      .st_hero-section_right_text_feature-item--{{ block.id }} .st_hero-section_right_text_feature_text,
      .st_hero-section_right_text_feature-item--{{ block.id }} .st_hero-section_right_text_feature_text * {
        display: inline-block;
        font-size: {{ block.settings.feature_size }}px;
        color: {{ block.settings.feature_color }};
        font-family: {{ block.settings.feature_font.family }}, {{ block.settings.feature_font.fallback_families }};
        font-weight: {{ block.settings.feature_font.weight }};
        margin: 0;
      }

      .st_hero-section_right_text_feature-item--{{ block.id }} .st_hero-section_right_text_feature_text strong {
        font-weight: bold;
      }

      .st_hero-section_right_text_feature-item--{{ block.id }} .st_hero-section_right_text_feature_text em {
        font-style: italic;
      }

      .st_hero-section_right_text_check-icon--{{ block.id }} {
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-right: 10px;
        flex-shrink: 0;
      }

      .st_hero-section_right_text_check-icon--{{ block.id }} img {
        width: 100%;
        height: {{ block.settings.icon_size }}px;
        object-fit: contain;
        opacity: {{ block.settings.icon_opacity }};
        flex-shrink: 0;
      }

      .st_hero-section_right_text_check-icon--{{ block.id }} svg {
        width: {{ block.settings.icon_size }}px;
        height: {{ block.settings.icon_size }}px;
      }

      @media screen and (max-width: 768px) {
        .st_hero-section_right_text_feature-item--{{ block.id }},
        .st_hero-section_right_text_feature-item--{{ block.id }} .st_hero-section_right_text_feature_text,
        .st_hero-section_right_text_feature-item--{{ block.id }} .st_hero-section_right_text_feature_text * {
          font-size: {{ block.settings.mobile_feature_size }}px;
        }
      }
    {% endif %}
  {% endfor %}

  /* Badge block styles */
  {% for block in section.blocks %}
    {% if block.type == 'badge' %}
      {{ block.settings.font | font_face }}
      .st_hero-section_right_text_badge-{{ block.id }} {
        display: inline-block;
        background-color: {{ block.settings.background_color }};
        color: {{ block.settings.text_color }};
        padding: {{ block.settings.padding_vertical }}px {{ block.settings.padding_horizontal }}px;
        border-radius: {{ block.settings.border_radius }}px;
        font-size: {{ block.settings.font_size }}px;
        font-family: {{block.settings.font.family}}, {{block.settings.font.fallback_families}};
        font-weight: {{block.settings.font.weight}};
      }

      @media screen and (max-width: 768px) {
        .st_hero-section_right_text_badge-{{ block.id }} {
          font-size: {{ block.settings.mobile_font_size }}px;
        }
      }
    {% endif %}
  {% endfor %}

  /* Button block styles */
  {% for block in section.blocks %}
    {% if block.type == 'button' %}
      {{ block.settings.heading_font | font_face }}

      .st_hero-section_right_text_button_container-{{ block.id }} {
        width: 100%;
        display: flex;
        {% if block.settings.desktop_alignment == 'left' %}
        justify-content: flex-start;
        {% elsif block.settings.desktop_alignment == 'right' %}
        justify-content: flex-end;
        {% else %}
        justify-content: center;
        {% endif %}
      }

      .st_hero-section_right_text_button_text-{{ block.id }} span,
      .st_hero-section_right_text_button_text-{{ block.id }} span *,
      .st_hero-section_right_text_button_text-{{ block.id }} span strong {
        font-size: {{block.settings.font_size}}px;
        color: {{block.settings.font_color}};
        font-family: {{block.settings.heading_font.family}}, {{block.settings.heading_font.fallback_families}};
        line-height: {{block.settings.line_height}}%;
        font-weight: {{block.settings.heading_font.weight}};
        font-style: {{block.settings.heading_font.style}};
      }

      .st_hero-section_right_text_button_text-{{block.id}} {
        display: flex;
        justify-content: center;
        align-items: center;
        gap: {{block.settings.gap_size}}px;
      }

      .st_hero-section_right_text_button-{{block.id}} {
        background-color: {{block.settings.button_color}};
        border-radius: {{block.settings.border_radius}}px;
        padding: 15px 25px;
        display: flex;
        justify-content: center;
        align-items: center;
        width: fit-content;
        cursor: pointer;
        transition: all 0.5s ease 0s;
      }

      .st_hero-section_right_text_button_text-{{ block.id }} svg {
        fill: {{block.settings.icon_color}};
        width: {{ block.settings.icon_size }}px;
        height: {{ block.settings.icon_size }}px;
        {% if block.settings.button_icon_animation %}
          opacity: 0;
          width: 0;
          transform: translateX(-100%);
          transition: all 0.5s ease 0s;
        {% endif %}
      }

      .st_hero-section_right_text_button_img-{{block.id}} {
        width: {{ block.settings.icon_size }}px;
        height: {{ block.settings.icon_size }}px;
        object-fit: contain;
        {% if block.settings.button_icon_animation %}
          opacity: 0;
          width: 0;
          transform: translateX(-100%);
          transition: all 0.5s ease 0s;
        {% endif %}
      }

      @media screen and (max-width: 768px) {
        .st_hero-section_right_text_button_container-{{ block.id }} {
          {% if block.settings.mobile_alignment == 'left' %}
          justify-content: flex-start;
          {% elsif block.settings.mobile_alignment == 'right' %}
          justify-content: flex-end;
          {% elsif block.settings.mobile_alignment == 'full' %}
          justify-content: stretch;
          {% else %}
          justify-content: center;
          {% endif %}
          max-width: 400px;
          margin: 0 auto;
        }

        .st_hero-section_right_text_button-{{block.id}} {
          {% if block.settings.mobile_alignment == 'full' %}
          width: 100%;
          {% endif %}
        }

        .st_hero-section_right_text_button_text-{{ block.id }} span,
        .st_hero-section_right_text_button_text-{{ block.id }} span *,
        .st_hero-section_right_text_button_text-{{ block.id }} span strong {
          font-size: {{ block.settings.mobile_font_size }}px;
        }
      }

      {% if block.settings.button_icon_animation %}
      .st_hero-section_right_text_button-{{block.id}}:hover .st_hero-section_right_text_button_img-{{block.id}},
      .st_hero-section_right_text_button-{{block.id}}:hover .st_hero-section_right_text_button_text-{{ block.id }} svg {
        opacity: 1;
        width: {{ block.settings.icon_size }}px;
        transform: translateX(0);
      }
      {% endif %}
    {% endif %}
  {% endfor %}

  /* Social Proof block styles */
  {% for block in section.blocks %}
    {% if block.type == 'social_proof' %}
      {{ block.settings.testimonial_font | font_face }}
      {{ block.settings.name_font | font_face }}

      .st_hero-section_right_text_social_proof-{{ block.id }} {
        display: flex;
        align-items: flex-start;
        gap: 15px;
        background-color: {{ block.settings.background_color }};
        border-radius: {{ block.settings.border_radius }}px;
        padding: {{ block.settings.padding }}px;
        width: 100%;
        max-width: 600px;
        {% if block.settings.show_border %}
          border: 1px solid {{ block.settings.border_color }};
        {% endif %}
      }

      .st_hero-section_right_text_social_proof_avatar {
        flex-shrink: 0;
      }

      .st_hero-section_right_text_social_proof_avatar_img {
        width: {{ block.settings.avatar_size }}px;
        height: {{ block.settings.avatar_size }}px;
        border-radius: 50%;
        object-fit: cover;
      }

      .st_hero-section_right_text_social_proof_avatar_placeholder {
        width: {{ block.settings.avatar_size }}px;
        height: {{ block.settings.avatar_size }}px;
        border-radius: 50%;
        background-color: #E0E0E0;
      }

      .st_hero-section_right_text_social_proof_content {
        display: flex;
        flex-direction: column;
        gap: 8px;
        flex: 1;
      }

      .st_hero-section_right_text_social_proof_testimonial {
        font-family: {{ block.settings.testimonial_font.family }}, {{ block.settings.testimonial_font.fallback_families }};
        font-size: {{ block.settings.testimonial_size }}px;
        color: {{ block.settings.testimonial_color }};
        font-weight: {{ block.settings.testimonial_font.weight }};
        line-height: 1.4;
      }

      .st_hero-section_right_text_social_proof_testimonial p {
        margin: 0;
        font-family: inherit;
        font-size: inherit;
        color: inherit;
        font-weight: inherit;
        line-height: inherit;
      }

      .st_hero-section_right_text_social_proof_testimonial strong {
        font-weight: bold;
      }

      .st_hero-section_right_text_social_proof_testimonial em {
        font-style: italic;
      }

      .st_hero-section_right_text_social_proof_footer {
        display: flex;
        flex-direction: row;
        align-items: center;
        gap: 10px;
      }

      .st_hero-section_right_text_social_proof_rating-{{ block.id }} {
        display: flex;
        gap: 2px;
      }

      .social-proof-rating-{{ block.id }} {
        width: calc({{ block.settings.star_size }}px * 5.5);
        max-height: {{ block.settings.star_size }}px;
      }

      .social-proof-rating__background-{{ block.id }} {
        fill: {{ block.settings.star_color_background }};
        stroke: none;
        stroke-width: 0;
        height: 100%;
        width: 100%;
      }

      .social-proof-rating__value-{{ block.id }} {
        fill: {{ block.settings.star_color }};
        height: 100%;
      }

      .st_hero-section_right_text_social_proof_customer_name {
        font-family: {{ block.settings.name_font.family }}, {{ block.settings.name_font.fallback_families }};
        font-size: {{ block.settings.name_size }}px;
        color: {{ block.settings.name_color }};
        font-weight: {{ block.settings.name_font.weight }};
      }

      @media screen and (max-width: 768px) {
        .st_hero-section_right_text_social_proof_testimonial {
          font-size: {{ block.settings.mobile_testimonial_size }}px;
        }

        .social-proof-rating-{{ block.id }} {
          width: calc({{ block.settings.star_size }}px * 5.5 * 0.9);
          max-height: calc({{ block.settings.star_size }}px * 0.9);
        }

        .st_hero-section_right_text_social_proof_customer_name {
          font-size: {{ block.settings.mobile_name_size }}px;
        }
      }
    {% endif %}
  {% endfor %}

  @media screen and (max-width: 860px) {
    .st_hero-section-{{ section.id }}{
      flex-direction: column;
      padding-inline: 0px;
    }

    .st_hero-section_wrapper-{{ section.id }}{
      flex-direction: column;
      border-radius: 0px;
    }

    .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left {
      width: 100%;
      {% if section.settings.mobile_layout == 'media_bottom' %}
      order: 1; /* Move this section to the bottom */
      {% else %}
      order: -1; /* Move this section to the top */
      {% endif %}
      min-height: 80vw;
    }

    .st_hero-section_wrapper-{{ section.id }} .st_hero-section_left img {
      aspect-ratio: 1/1;
      height: auto;
      max-height: none;
    }

    .st_hero-section_wrapper-{{ section.id }} .st_hero-section_right {
      width: 100%;
      padding: 15px;
      min-height: unset; /* Remove the min-height on mobile */
    }

    .st_hero-section_wrapper-{{ section.id }} .st_hero-section_right_text {
      width: 100%;
      min-height: unset;
      gap: {{ section.settings.content_gap | times: 0.75 | round }}px;
    }

    {% for block in section.blocks %}
      {% if block.type == 'title' %}
        .st_hero-section_wrapper-{{ section.id }} .st_hero-section_right_text_title-{{ block.id }},
        .st_hero-section_wrapper-{{ section.id }} .st_hero-section_right_text_title-{{ block.id }} .highlight {
          font-size: {{ block.settings.mobile_title_size }}px;
        }
      {% endif %}

      {% if block.type == 'description' %}
        .st_hero-section_wrapper-{{ section.id }} .st_hero-section_right_text_description-{{ block.id }} {
          font-size: calc({{ block.settings.font_size }}px / 1.2);
        }
      {% endif %}
    {% endfor %}
  }
</style>

<div class="st_hero-section-{{ section.id }} st_wrapper">
  <div class="st_hero-section_wrapper-{{ section.id }}">
    <div class="st_hero-section_left">
      <div class="st_hero-section_left_wrapper">
        {% if section.settings.enable_background %}
          {% if section.settings.media_type == 'image' %}
            {% if section.settings.image %}
              {{
                section.settings.image
                | image_url: height: section.settings.section_height
                | image_tag:
                  class: 'background-img',
                  width: section.settings.section_height,
                  height: section.settings.section_height
              }}
            {% else %}
              <img
                src="https://placehold.co/300x300.png"
                alt="Default Overlay"
                class="background-img"
                width="300"
                height="300"
              >
            {% endif %}
          {% else %}
            <div
              class="st_hero-section_left_gradient"
              style="background: {{ section.settings.media_gradient }}; width: 100%; height: 100%;"
            ></div>
          {% endif %}
        {% endif %}

        {% if section.settings.overlay_image_checkbox %}
          {% if section.settings.overlay_image %}
            {{
              section.settings.overlay_image
              | image_url: height: section.settings.section_height
              | image_tag:
                class: 'overlay-img',
                width: section.settings.section_height,
                height: section.settings.section_height
            }}
          {% else %}
            <img
              src="https://cdn.shopify.com/s/files/1/0630/4923/6640/files/Frame_279.png?height=850&v=1744313231&width=850"
              alt="Default Overlay"
              class="overlay-img"
              width="300"
              height="300"
            >
          {% endif %}
        {% endif %}
      </div>
    </div>

    <div class="st_hero-section_right">
      <div class="st_hero-section_right_text">
        {% assign in_list = false %}

        {% for block in section.blocks %}
          {% comment %}Close list if needed when changing block types{% endcomment %}
          {% if in_list and block.type != 'icon_and_text' %}
            </ul>
            {% assign in_list = false %}
          {% endif %}

          {% case block.type %}
            {% when 'star_rating' %}
              <div class="st_hero-section_right_text_star_rating-{{ block.id }}" {{ block.shopify_attributes }}>
                <div class="st_hero-section_right_text_star_rating-stars-{{ block.id }}">
                  <svg viewBox="0 0 1000 200" class="rating-{{ block.id }}">
                    <defs>
                      <polygon id="star-{{ block.id }}" points="100,0 131,66 200,76 150,128 162,200 100,166 38,200 50,128 0,76 69,66"></polygon>
                      <clipPath id="stars-{{ block.id }}">
                        <use xlink:href="#star-{{ block.id }}"/>
                        <use xlink:href="#star-{{ block.id }}" x="20%"/>
                        <use xlink:href="#star-{{ block.id }}" x="40%"/>
                        <use xlink:href="#star-{{ block.id }}" x="60%"/>
                        <use xlink:href="#star-{{ block.id }}" x="80%"/>
                      </clipPath>
                    </defs>
                    <rect class="rating__background-{{ block.id }}" clip-path="url(#stars-{{ block.id }})"></rect>
                    <rect width="{{ block.settings.rating | times: 20 }}%" class="rating__value-{{ block.id }}" clip-path="url(#stars-{{ block.id }})"></rect>
                  </svg>
                </div>
                <span>{{ block.settings.rating_text }}</span>
              </div>

            {% when 'title' %}
              <h2 class="st_hero-section_right_text_title-{{ block.id }}" {{ block.shopify_attributes }}>
                {{ block.settings.heading_text | replace: '[', '<div class="highlight">' | replace: ']', '</div>' }}
              </h2>

            {% when 'description' %}
              <div class="st_hero-section_right_text_description-{{ block.id }}" {{ block.shopify_attributes }}>
                {{ block.settings.heading | replace: '[', '<st>' | replace: ']', '</st>' }}
              </div>

            {% when 'icon_and_text' %}
              {% unless in_list %}
                <ul class="st_hero-section_right_text_features-container">
                  {% assign in_list = true %}
              {% endunless %}

              <li class="st_hero-section_right_text_feature-item--{{ block.id }}" {{ block.shopify_attributes }}>
                <div class="st_hero-section_right_text_check-icon st_hero-section_right_text_check-icon--{{ block.id }}">
                  {% if block.settings.icon != blank %}
                    <img
                      src="{{ block.settings.icon | image_url: width: 50 }}"
                      width="50"
                      height="50"
                      alt=""
                      loading="lazy"
                    >
                  {% else %}
                    <svg xmlns="http://www.w3.org/2000/svg" width="23" height="23" viewBox="0 0 23 23" fill="none">
                      <path d="M19 11C19 14.866 15.866 18 12 18C8.13401 18 5 14.866 5 11C5 7.13401 8.13401 4 12 4C15.866 4 19 7.13401 19 11Z" fill="{{ block.settings.icon_color }}"/>
                      <path d="M11.5 0C9.22552 0 7.00211 0.674463 5.11095 1.9381C3.21978 3.20173 1.7458 4.99779 0.87539 7.09914C0.00498277 9.20049 -0.222756 11.5128 0.220974 13.7435C0.664704 15.9743 1.75997 18.0234 3.36828 19.6317C4.97658 21.24 7.02568 22.3353 9.25646 22.779C11.4872 23.2228 13.7995 22.995 15.9009 22.1246C18.0022 21.2542 19.7983 19.7802 21.0619 17.8891C22.3255 15.9979 23 13.7745 23 11.5C22.9968 8.451 21.7841 5.5278 19.6282 3.37183C17.4722 1.21586 14.549 0.0032198 11.5 0ZM16.5489 9.47202L10.3566 15.6643C10.2745 15.7466 10.1769 15.8118 10.0695 15.8563C9.96214 15.9009 9.84702 15.9238 9.73077 15.9238C9.61452 15.9238 9.49941 15.9009 9.39202 15.8563C9.28463 15.8118 9.18706 15.7466 9.10491 15.6643L6.45106 13.0105C6.28507 12.8445 6.19182 12.6194 6.19182 12.3846C6.19182 12.1499 6.28507 11.9247 6.45106 11.7587C6.61705 11.5928 6.84218 11.4995 7.07693 11.4995C7.31167 11.4995 7.5368 11.5928 7.70279 11.7587L9.73077 13.7878L15.2972 8.22029C15.3794 8.1381 15.477 8.0729 15.5844 8.02842C15.6917 7.98394 15.8068 7.96104 15.9231 7.96104C16.0393 7.96104 16.1544 7.98394 16.2618 8.02842C16.3692 8.0729 16.4668 8.1381 16.5489 8.22029C16.6311 8.30248 16.6963 8.40005 16.7408 8.50744C16.7853 8.61482 16.8082 8.72992 16.8082 8.84615C16.8082 8.96239 16.7853 9.07748 16.7408 9.18487C16.6963 9.29225 16.6311 9.38983 16.5489 9.47202Z" fill="{{ block.settings.icon_background }}"/>
                    </svg>
                  {% endif %}
                </div>
                <div class="st_hero-section_right_text_feature_text">
                  {{ block.settings.feature_text }}
                </div>
              </li>

            {% when 'badge' %}
              <div class="st_hero-section_right_text_badge-{{ block.id }}" {{ block.shopify_attributes }}>
                {{ block.settings.label_text }}
              </div>

            {% when 'button' %}
              <div class="st_hero-section_right_text_button_container-{{ block.id }}">
                <a
                  href="{{ block.settings.button_link }}"
                  {% if block.settings.mobile_alignment == 'full' %}
                    style="width: 100%;"
                  {% endif %}
                >
                  <button class="st_hero-section_right_text_button-{{ block.id }}" {{ block.shopify_attributes }}>
                    <div class="st_hero-section_right_text_button_text-{{ block.id }}">
                      <span>{{ block.settings.button_text }}</span>
                      {% if block.settings.toggle_icon %}
                        {% if block.settings.image %}
                          <img
                            src="{{ block.settings.image | image_url: width: 32 }}"
                            class="st_hero-section_right_text_button_img-{{ block.id }}"
                            alt=""
                            width="{{ block.settings.icon_size }}"
                            height="{{ block.settings.icon_size }}"
                          >
                        {% else %}
                          <svg
                            xmlns="http://www.w3.org/2000/svg"
                            viewBox="0 0 256 256"
                          >
                            <path d="M221.66,133.66l-72,72a8,8,0,0,1-11.32-11.32L196.69,136H40a8,8,0,0,1,0-16H196.69L138.34,61.66a8,8,0,0,1,11.32-11.32l72,72A8,8,0,0,1,221.66,133.66Z"/>
                          </svg>
                        {% endif %}
                      {% endif %}
                    </div>
                  </button>
                </a>
              </div>
            {% when 'social_proof' %}
              <div class="st_hero-section_right_text_social_proof-{{ block.id }}" {{ block.shopify_attributes }}>
                <div class="st_hero-section_right_text_social_proof_avatar">
                  {% if block.settings.avatar %}
                    {{
                      block.settings.avatar
                      | image_url: width: block.settings.avatar_size
                      | image_tag:
                        class: 'st_hero-section_right_text_social_proof_avatar_img',
                        width: block.settings.avatar_size,
                        height: block.settings.avatar_size
                    }}
                  {% else %}
                    <div
                      class="st_hero-section_right_text_social_proof_avatar_placeholder"
                      style="width: {{ block.settings.avatar_size }}px; height: {{ block.settings.avatar_size }}px;"
                    ></div>
                  {% endif %}
                </div>
                <div class="st_hero-section_right_text_social_proof_content">
                  <div class="st_hero-section_right_text_social_proof_testimonial">
                    {{ block.settings.testimonial }}
                  </div>
                  <div class="st_hero-section_right_text_social_proof_footer">
                    <div class="st_hero-section_right_text_social_proof_rating-{{ block.id }}">
                      <svg viewBox="0 0 1000 200" class="social-proof-rating-{{ block.id }}">
                        <defs>
                          <polygon id="social-proof-star-{{ block.id }}" points="100,0 131,66 200,76 150,128 162,200 100,166 38,200 50,128 0,76 69,66"></polygon>
                          <clipPath id="social-proof-stars-{{ block.id }}">
                            <use xlink:href="#social-proof-star-{{ block.id }}"/>
                            <use xlink:href="#social-proof-star-{{ block.id }}" x="20%"/>
                            <use xlink:href="#social-proof-star-{{ block.id }}" x="40%"/>
                            <use xlink:href="#social-proof-star-{{ block.id }}" x="60%"/>
                            <use xlink:href="#social-proof-star-{{ block.id }}" x="80%"/>
                          </clipPath>
                        </defs>
                        <rect class="social-proof-rating__background-{{ block.id }}" clip-path="url(#social-proof-stars-{{ block.id }})"></rect>
                        <rect width="{{ block.settings.rating | times: 20 }}%" class="social-proof-rating__value-{{ block.id }}" clip-path="url(#social-proof-stars-{{ block.id }})"></rect>
                      </svg>
                    </div>
                    <div class="st_hero-section_right_text_social_proof_customer_name">
                      {{ block.settings.customer_name }}
                    </div>
                  </div>
                </div>
              </div>
            {% when '@app' %}
              {% render block %}
          {% endcase %}
        {% endfor %}

        {% if in_list %}
          </ul>
        {% endif %}
      </div>
    </div>
  </div>
</div>

